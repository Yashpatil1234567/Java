
public class Maximum {

	public static void main(String[] args) {
		int a=12;
		int b=50;
		int c=9;
		int max=0;
		
		if(a>b) {
			if(a>c) {
				max=a;
			}
			else{
				max=c;
			}
		}
		else{
			if(b>c) {
				max=b;
			}
			else {
				max=c;
			}
		}
		
		System.out.println("maximum number is"+max);
		}
				
		
		
	}


